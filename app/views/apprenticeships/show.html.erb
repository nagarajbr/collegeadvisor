<div class="page-header">


	 <%= link_to interest_profilers_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>

  <h1>Apprenticeship Listings For <%= params[:id] %></h1>
</div>

<table class="table table-striped table-bordered">
        <thead>
              <tr>
              <th> Title </th>
              <th> Sponsor Name </th>
              <th> Address </th>
              <th> Phone </th>
              <th> Referral </th>
            </tr>
            </thead>
          <tbody>
            <% if !@apprenticeship_listings_array.empty? %>
               <% @apprenticeship_listings_array.each do |key,val| %>
                <tr>
                <td><%= key[:title] %></td>
                <td> <%= key[:sponsor] %></td>
                <td> <%= key[:address] %></td>
                <td> <%= key[:phone] %></td>
                <td> <%= link_to "Referral", "#" ,id: "hyperlink" ,target: :_blank %></td>

              </tr>
            <% end %>
            <% end %>
        </tbody>
      </table>