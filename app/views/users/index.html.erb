
<div class="page-header">
 
  <%= link_to new_user_invitation_path , class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New User
  <% end %>
  <%= link_to "Upload a batch of users", new_batch_invitation_path, class: "btn btn-primary" %>

  <h1>Users</h1>
</div>
<div class="container">
  <div class="row">
    
    <div class="column">
      <table class="table table-striped table-bordered table-hover">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
