<fieldset>
  <section class="solidbody">

<legend> <%= set_subheader("Work Interests") %> </legend>
<%= form_for @quest, url: next_wizard_path, :method => :get do |f| %>
<% question = 1 %>
<table class="table table-striped table-bordered">
				<thead>
					    <tr>
							<th width="5%"> </th>

							<th width="11%"> 
								
                                <%= image_tag("strongly_dislike.png" )  %> <%= "Strongly Dislike" %>
                                
								
							</th>
							<th width="11%"> <%= image_tag("dislike.png") %> <%= "Dislike" %>  </th>
							<th width="11%"> <%= image_tag("unsure.png") %> <%= "Unsure" %> </th>
							<th width="11%"> <%= image_tag("like.png") %> <%= "Like" %> </th>
							<th width="11%"> <%= image_tag("strongly_like.png") %> <%= "Strongly Like" %> </th>
							<th width="45%">  </th>
						</tr>
					  </thead>
			    <tbody>
			    	<% question = 1 %>
			    	<% answer = 0 %>
			    	<% @questions.each do |key,val| %>
			    	<% if (31..40).include?(question) %>
			    		<tr>
							<td> <%= question %></td>
							<% count = 1 %>
							<% ["Strongly Dislike", "Dislike" ,"Unsure","Like","Strongly Like"].each do |key1| %>
							<% if session[params[:id]].present? %>
								<%  if count == session[params[:id]].at(answer).to_i  %>
	                            <td> <%= radio_button_tag(question, count,checked = true ) %></td>
	                            <% else %>
	                            <td> <%= radio_button_tag(question , count) %></td>
	                            <% end %>
	                        <% else %>
	                        <td> <%= radio_button_tag(question , count) %></td>
	                        <% end %>
                            <% count += 1 %>
							<% end %>

							<td> <%= key["interest"] %></td>

     					</tr>
     					<% answer += 1 %>
                        	<% end %>
						<% question += 1 %>
					<% end %>
				</tbody>
			</table>
			<div class="row">
			<%= link_to "Prev", previous_wizard_path, :class => 'btn btn-primary' %>
			<%= f.submit "Next", :class => 'btn btn-primary' %>
			</div>
<% end %>



 </section>
</fieldset>
