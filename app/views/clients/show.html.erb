<div class="page-header">
  <%= link_to clients_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_client_path(@client), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show client</h1>
</div>

<dl class="dl-horizontal">
  <dt>First name:</dt>
  <dd><%= @client.first_name %></dd>

  <dt>Last name:</dt>
  <dd><%= @client.last_name %></dd>

  <dt>Middle name:</dt>
  <dd><%= @client.middle_name %></dd>

  <dt>Suffix:</dt>
  <dd><%= @client.suffix %></dd>

  <dt>Dob:</dt>
  <dd><%= @client.dob %></dd>

  <dt>Gender:</dt>
  <dd><%= @client.gender %></dd>

</dl>
