<fieldset>
  <section class="solidbody">

<legend> <%= set_subheader("Client Details") %> </legend><br>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <% @clients.attributes.each do |key,val| %>
      <tr>
            <th><%= key.titleize %></th>
            <td><%= val %></td>
       </tr>
      <% end %>
    </thead>
  </table>
</div>
 </section>
</fieldset>



