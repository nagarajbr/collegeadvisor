<div class="page-header">


	 <%= link_to interest_profilers_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>

  <h1>Training Listings For <%= params["keyword"].present? ? params["keyword"] : params[:id] %></h1>
</div>

<table class="table table-striped table-bordered">
        <thead>
              <tr>
              <th> SchoolName</th>
              <th> Address </th>
              <th> Phone </th>
              <th> Program Name </th>
              <th> Program Length </th>
              <th> Referral </th>
            </tr>
            </thead>
          <tbody>
            <% if !@training_listing_array.empty? %>
               <% @training_listing_array.each do |key,val| %>
                <tr>
                <td><%= key[:schoolName] %></td>
                <td> <%= key[:address] %></td>
                <td> <%= key[:phone] %></td>
                <td> <%= key[:ProgramName] %></td>
                <td> <%= key[:ProgramLength] %></td>
                <td> <%= link_to "Referral", key[:url] , id: "hyperlink" ,target: :_blank %></td>
                </tr>
            <% end %>
            <% end %>
        </tbody>
      </table>