<div class="page-header">
  <%= link_to addresses_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_address_path(@address), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show address</h1>
</div>

<dl class="dl-horizontal">
  <dt>Address1:</dt>
  <dd><%= @address.address1 %></dd>

  <dt>Address2:</dt>
  <dd><%= @address.address2 %></dd>

  <dt>City:</dt>
  <dd><%= @address.city %></dd>

  <dt>State:</dt>
  <dd><%= @address.state %></dd>

  <dt>Zip:</dt>
  <dd><%= @address.zip %></dd>

  <dt>Address type:</dt>
  <dd><%= @address.address_type %></dd>

</dl>
